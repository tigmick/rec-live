  <% @clients = User.where(:role=> "client")%>
  <table class="table respon">
    <thead>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
	<th colspan=2 style="text-align:center;">Actions</th>
      </tr>
    </thead>
    <tbody>
      <% @clients.each do |client|%>
      <tr>
        <td><%= client.first_name %></td>
        <td><%= client.first_name %></td>
        <td><%= client.email %></td>
	<td><%= link_to "view","/admin/users/#{client.id}"%>
	<td><%= link_to "Job Created", admin_client_jobs_path(client) %></td>
      </tr>
      <% end %>
    </tbody>
      
  </table>


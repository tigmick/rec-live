<%= form_tag next_step_interview_schedules_path do %>
	<H3>Next Step</H3>
	<%= hidden_field_tag :scheds_id,'' %>
	<div class="form-group">
	  <%= label_tag :next_step, nil, class: "col-md-3 control-label" %>
    <div class="col-md-9">
    	<%= number_field_tag :next_step,nil, class: "result form-control"%>
    </div>
  </div>
  <div class="form-group">
	  <%= label_tag "Next Step Description", nil, class: "col-md-3 control-label" %>
    <div class="col-md-9">
    	<%= text_area_tag :next_step_desc,nil, class: "result form-control"%>
    </div>
  </div>
  <div class="form-group">
	  <div class="col-md-9">
	  	<%= submit_tag "submit",class: "btn btn-primary"%>
	  </div>
  </div>
 <% end %>